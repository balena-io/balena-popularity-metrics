{
  "name": "@balena/popularity-metrics",
  "private": true,
  "description": "Balena popularity metrics",
  "main": "src/index",
  "version": "0.0.6",
  "scripts": {
    "test": "echo Tests are run via the docker build",
    "test:in-container": "npm run lint && jest --silent --coverage --runInBand --collectCoverageFrom='src/**/*.{ts,jxs}'",
    "prettify": "balena-lint --fix src/ test/",
    "lint": "balena-lint src/ test/ && tsc --noEmit",
    "dev": "ts-node-dev src/index",
    "build": "tsc"
  },
  "author": "balena.io",
  "license": "ISC",
  "devDependencies": {
    "@balena/lint": "^6.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "husky": "^8.0.2",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@balena/node-metrics-gatherer": "^6.0.3",
    "@types/node": "^18.11.9",
    "balena-sdk": "^16.32.2",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": "^18.12.0",
    "npm": "^8.19.2"
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ],
    "test/**/*.ts": [
      "balena-lint --no-prettier --tests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "versionist": {
    "publishedAt": "2023-02-10T18:28:56.421Z"
  }
}
